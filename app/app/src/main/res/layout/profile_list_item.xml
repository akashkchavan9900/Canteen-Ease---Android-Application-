<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        android:id="@+id/tvn_sname"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:background="@color/colorPrimaryDark"
        android:textAllCaps="true"
        android:text="Suyog jadhav"
        android:fontFamily="@font/sanspro"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/tvn_scollageid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        android:textStyle="bold"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:layout_gravity="center"
        android:layout_marginTop="8dp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="horizontal">

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Gender : "
            android:fontFamily="@font/sanspro"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="20dp"/>
        <TextView
            android:id="@+id/tvn_sgender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:fontFamily="@font/sanspro"
            android:layout_marginLeft="10dp"
            android:textSize="20dp"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="horizontal">

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bithdate : "
            android:fontFamily="@font/sanspro"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:textSize="20dp"/>
        <TextView
            android:id="@+id/tvn_sbirthdate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/sanspro"
            android:layout_marginLeft="10dp"
            android:textSize="20dp"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="horizontal">

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mobile No : "
            android:fontFamily="@font/sanspro"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="20dp"/>
        <TextView
            android:id="@+id/tvn_smnumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:fontFamily="@font/sanspro"
            android:layout_marginLeft="10dp"
            android:textSize="20dp"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="horizontal">

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Address : "
            android:fontFamily="@font/sanspro"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="20dp"/>
        <TextView
            android:id="@+id/tvn_saddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:fontFamily="@font/sanspro"
            android:layout_marginLeft="10dp"
            android:textSize="20dp"/>

    </LinearLayout>
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    android:orientation="horizontal">

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Department"
            android:fontFamily="@font/sanspro"
            android:textColor="@color/black"
            android:textSize="20dp"
            android:textStyle="bold" />
    <TextView
        android:id="@+id/tvn_sdepartment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:fontFamily="@font/sanspro"
        android:layout_marginLeft="10dp"
        android:textSize="20dp"/>

</LinearLayout>
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    android:orientation="horizontal">

    <TextView

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Acedemic Year : "
        android:fontFamily="@font/sanspro"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/tvn_sacadamic_year"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:fontFamily="@font/sanspro"
        android:layout_marginLeft="10dp"
        android:textSize="20dp"/>

</LinearLayout>
</LinearLayout>



    <TextView
        android:id="@+id/tvn_wallet"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:background="@color/colorPrimaryDark"
        android:textAllCaps="true"
        android:text="Suyog jadhav"
        android:layout_alignParentBottom="true"
        android:fontFamily="@font/sanspro"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold" />

</RelativeLayout>